.nav
    background-color: #EEFBFF    
    width: 100%
    height: 110px
    padding: 0px
    margin: 0px
    display: inline-block
    position: fixed
    z-index: 100
    
   
    
    img  
      width: 110px
      margin: 6px 0px 0px 0px 
      display:inline
      padding: 0px 0px 0px 20px
               
    
    .list
        padding: 20px  300px 20px
        margin: 50px 0px 20px 0px
        display: inline
        
        
        
        ul   
            display: inline
            padding: 5px 0px 0px 0px
            margin: 50px 0px 10px 0px
            bottom:0px
            position:absolute
            text-decoration:none
            
          
            
            li   
                display: inline
                font-size: 20px
                font-weight: 500
                color:#F9C029  
                padding: 10px 10px 0px 0px 
                margin:10px
                text-decoration:none
                
                a    
                    text-decoration:none
                    color:#F9C029 
            li:hover 
               background-color: #D9F441      
               padding: 12px
               border-radius:5px
               cursor: pointer
               